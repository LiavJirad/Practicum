   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "def score_effect(platform, scoring):\n",
    "    data_clean = data_new.query('platform == @platform')[['total_sales', scoring]]\n",
    "    sb.scatterplot(data = data_clean, x = scoring, y = 'total_sales')\n",
    "    print(data_clean.corr())\n",
    "    plt.show()"
   ]
  },
  {
